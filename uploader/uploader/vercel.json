{

    "builds": [{

        "src": "uploader/wsgi.py",

        "use": "@vercel/python",

        "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }

    }],

    "routes": [

        {

            "src": "/(.*)",

            "dest": "uploader/wsgi.py"

        }

    ]

}